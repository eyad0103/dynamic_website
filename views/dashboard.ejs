    <style>
        /* PC List Styles */
        .pc-list {
            display: grid;
            gap: 1rem;
        }
        
        .pc-item {
            background: rgba(0, 0, 0, 0.4);
            border: 1px solid rgba(0, 255, 136, 0.2);
            border-radius: 8px;
            padding: 1.5rem;
            transition: all 0.3s ease;
        }
        
        .pc-item:hover {
            border-color: rgba(0, 255, 136, 0.5);
            background: rgba(0, 0, 0, 0.6);
            transform: translateY(-2px);
        }
        
        .pc-header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            margin-bottom: 1rem;
        }
        
        .pc-info h4 {
            margin: 0;
            color: #00ff88;
            font-size: 1.2rem;
        }
        
        .pc-location {
            color: rgba(255, 255, 255, 0.6);
            font-size: 0.9rem;
        }
        
        .pc-status {
            display: flex;
            align-items: center;
            gap: 0.5rem;
        }
        
        .status-online {
            color: #00ff88;
            font-weight: bold;
        }
        
        .status-offline {
            color: #ff6b6b;
            font-weight: bold;
        }
        
        .pc-details {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
            gap: 0.5rem;
            margin-bottom: 1rem;
        }
        
        .pc-detail {
            color: rgba(255, 255, 255, 0.8);
            font-size: 0.9rem;
        }
        
        .pc-actions {
            display: flex;
            gap: 0.5rem;
            flex-wrap: wrap;
        }
        
        .btn-small {
            padding: 0.4rem 0.8rem;
            font-size: 0.8rem;
            border: none;
            border-radius: 4px;
            cursor: pointer;
            font-weight: bold;
            transition: all 0.3s ease;
        }
        
        .btn-primary {
            background: #00ff88;
            color: #000;
        }
        
        .btn-primary:hover {
            background: #00cc70;
        }
        
        .btn-danger {
            background: #ff6b6b;
            color: #fff;
        }
        
        .btn-danger:hover {
            background: #ff5252;
        }
        
        .notification {
            position: fixed;
            top: 20px;
            right: 20px;
            z-index: 1000;
            min-width: 300px;
            max-width: 400px;
            border-radius: 8px;
            box-shadow: 0 6px 20px rgba(0, 0, 0, 0.4);
            animation: slideIn 0.3s ease;
            border: 2px solid rgba(255, 255, 255, 0.2);
            margin-bottom: 20px;
            backdrop-filter: blur(10px);
        }
        
        .notification-success {
            background: linear-gradient(135deg, #00ff88, #00cc70);
            color: #fff;
            border-color: #00cc70;
            text-shadow: 0 1px 3px rgba(0, 0, 0, 0.7);
            font-weight: bold;
        }
        
        .notification-error {
            background: linear-gradient(135deg, #ff4757, #ff3838);
            color: #fff;
            border-color: #ff3838;
            text-shadow: 0 1px 2px rgba(0, 0, 0, 0.5);
        }
        
        .notification-warning {
            background: linear-gradient(135deg, #ffa502, #ff8c00);
            color: #fff;
            border-color: #ff8c00;
            text-shadow: 0 1px 3px rgba(0, 0, 0, 0.7);
            font-weight: bold;
        }
        
        .notification-info {
            background: linear-gradient(135deg, #5f9ea0, #4682b4);
            color: #fff;
            border-color: #4682b4;
            text-shadow: 0 1px 2px rgba(0, 0, 0, 0.5);
        }
        
        .notification-content {
            padding: 1rem;
            position: relative;
            font-family: 'Courier New', monospace;
        }
        
        .notification-content h4 {
            margin: 0 0 0.5rem 0;
            font-size: 1rem;
            font-weight: 600;
            font-family: 'Courier New', monospace;
        }
        
        .notification-content p {
            margin: 0;
            font-size: 0.9rem;
            line-height: 1.4;
            font-family: 'Courier New', monospace;
        }
        
        .notification-close {
            position: absolute;
            top: 0.5rem;
            right: 0.5rem;
            background: rgba(255, 255, 255, 0.2);
            border: none;
            color: inherit;
            font-size: 1.2rem;
            cursor: pointer;
            opacity: 0.7;
            border-radius: 50%;
            width: 24px;
            height: 24px;
            display: flex;
            align-items: center;
            justify-content: center;
            transition: all 0.2s ease;
        }
        
        .notification-close:hover {
            opacity: 1;
            background: rgba(255, 255, 255, 0.3);
        }
        
        @keyframes slideIn {
            from {
                transform: translateX(100%);
                opacity: 0;
            }
            to {
                transform: translateX(0);
                opacity: 1;
            }
        }
        
        @keyframes slideOut {
            from {
                transform: translateX(0);
                opacity: 1;
            }
            to {
                transform: translateX(100%);
                opacity: 0;
            }
        }
        .dashboard-container {
            min-height: 100vh;
            background: linear-gradient(135deg, #0a0a0a 0%, #1a1a2e 100%);
            color: #00ff88;
            font-family: 'Courier New', monospace;
            position: relative;
            overflow: hidden;
        }

        .dashboard-container::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background-image: 
                repeating-linear-gradient(0deg, transparent, transparent 2px, rgba(0, 255, 136, 0.03) 2px, rgba(0, 255, 136, 0.03) 4px),
                repeating-linear-gradient(90deg, transparent, transparent 2px, rgba(0, 255, 136, 0.03) 2px, rgba(0, 255, 136, 0.03) 4px);
            background-size: 50px 50px;
            pointer-events: none;
            z-index: 1;
        }

        .dashboard-header {
            background: rgba(0, 0, 0, 0.9);
            border-bottom: 2px solid #00ff88;
            padding: 1rem 0;
            position: relative;
            z-index: 10;
            backdrop-filter: blur(10px);
        }

        .header-content {
            max-width: 1400px;
            margin: 0 auto;
            padding: 0 2rem;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        .dashboard-title {
            font-size: 1.8rem;
            font-weight: bold;
            text-transform: uppercase;
            letter-spacing: 3px;
            text-shadow: 0 0 15px #00ff88;
            animation: glow 2s ease-in-out infinite alternate;
        }

        @keyframes glow {
            from { text-shadow: 0 0 15px #00ff88; }
            to { text-shadow: 0 0 25px #00ff88, 0 0 35px #00ff88; }
        }

        .status-indicator {
            display: flex;
            align-items: center;
            gap: 0.5rem;
            padding: 0.5rem 1rem;
            background: rgba(0, 255, 136, 0.1);
            border: 1px solid #00ff88;
            border-radius: 4px;
            font-size: 0.9rem;
        }

        .status-dot {
            width: 8px;
            height: 8px;
            background: #00ff88;
            border-radius: 50%;
            animation: pulse 2s infinite;
        }

        @keyframes pulse {
            0%, 100% { opacity: 1; }
            50% { opacity: 0.5; }
        }

        .error-stats {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
            gap: 1.5rem;
            margin-bottom: 2rem;
        }

        .stat-card {
            background: rgba(0, 20, 40, 0.8);
            border: 1px solid rgba(0, 255, 136, 0.3);
            border-radius: 8px;
            padding: 1.5rem;
            position: relative;
            backdrop-filter: blur(5px);
            transition: all 0.3s ease;
        }

        .stat-card:hover {
            border-color: #00ff88;
            box-shadow: 0 0 20px rgba(0, 255, 136, 0.3);
            transform: translateY(-2px);
        }

        .stat-label {
            font-size: 0.8rem;
            color: #00ff88;
            text-transform: uppercase;
            letter-spacing: 1px;
            margin-bottom: 0.5rem;
            opacity: 0.8;
        }

        .stat-value {
            font-size: 1.8rem;
            font-weight: bold;
            color: #ffffff;
            text-shadow: 0 0 5px rgba(0, 255, 136, 0.5);
        }

        .error-list {
            background: rgba(0, 0, 0, 0.8);
            border: 1px solid #00ff88;
            border-radius: 8px;
            padding: 1.5rem;
            margin-bottom: 2rem;
        }

        .error-list-header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            margin-bottom: 1.5rem;
            padding-bottom: 1rem;
            border-bottom: 1px solid rgba(0, 255, 136, 0.3);
        }

        .error-list-title {
            font-size: 1.2rem;
            color: #00ff88;
            text-transform: uppercase;
            letter-spacing: 2px;
        }

        .error-filters {
            display: flex;
            gap: 1rem;
            align-items: center;
        }

        .filter-input {
            background: transparent;
            border: 1px solid rgba(0, 255, 136, 0.3);
            color: #00ff88;
            padding: 0.5rem;
            border-radius: 4px;
            font-family: 'Courier New', monospace;
            font-size: 0.9rem;
            outline: none;
        }

        .filter-input:focus {
            border-color: #00ff88;
            box-shadow: 0 0 10px rgba(0, 255, 136, 0.3);
        }

        .error-item {
            background: rgba(0, 255, 136, 0.05);
            border: 1px solid rgba(0, 255, 136, 0.2);
            border-radius: 4px;
            padding: 1rem;
            margin-bottom: 1rem;
            transition: all 0.3s ease;
            cursor: pointer;
        }

        .error-item:hover {
            background: rgba(0, 255, 136, 0.1);
            border-color: #00ff88;
            transform: translateX(5px);
        }

        .error-header {
            display: flex;
            justify-content: space-between;
            align-items: flex-start;
            margin-bottom: 0.5rem;
        }

        .error-app-name {
            font-size: 1.1rem;
            color: #ffffff;
            font-weight: bold;
        }

        .error-timestamp {
            font-size: 0.8rem;
            color: rgba(0, 255, 136, 0.6);
        }

        .error-status {
            display: inline-block;
            padding: 0.25rem 0.5rem;
            border-radius: 4px;
            font-size: 0.7rem;
            text-transform: uppercase;
            letter-spacing: 1px;
            margin-bottom: 0.5rem;
        }

        .status-pending {
            background: rgba(255, 193, 7, 0.2);
            color: #ffc107;
            border: 1px solid rgba(255, 193, 7, 0.3);
        }

        .status-analyzed {
            background: rgba(40, 167, 69, 0.2);
            color: #28a745;
            border: 1px solid rgba(40, 167, 69, 0.3);
        }

        .status-failed {
            background: rgba(220, 53, 69, 0.2);
            color: #dc3545;
            border: 1px solid rgba(220, 53, 69, 0.3);
        }

        .error-message {
            color: rgba(255, 255, 255, 0.8);
            font-size: 0.9rem;
            margin-bottom: 0.5rem;
            font-family: 'Courier New', monospace;
            background: rgba(0, 0, 0, 0.3);
            padding: 0.5rem;
            border-radius: 4px;
            border-left: 3px solid #00ff88;
        }

        .error-ai-explanation {
            color: #00ff88;
            font-size: 0.9rem;
            line-height: 1.4;
            background: rgba(0, 255, 136, 0.05);
            padding: 0.5rem;
            border-radius: 4px;
            border-left: 3px solid #00ff88;
            margin-top: 0.5rem;
        }

        .no-errors {
            text-align: center;
            color: rgba(0, 255, 136, 0.6);
            font-size: 1.1rem;
            padding: 2rem;
        }

        .loading {
            text-align: center;
            color: #00ff88;
            font-size: 1.1rem;
            padding: 2rem;
        }

        .refresh-btn {
            background: transparent;
            border: 1px solid #00ff88;
            color: #00ff88;
            padding: 0.5rem 1rem;
            border-radius: 4px;
            font-family: 'Courier New', monospace;
            cursor: pointer;
            transition: all 0.3s ease;
        }

        .refresh-btn:hover {
            background: rgba(0, 255, 136, 0.1);
            box-shadow: 0 0 10px rgba(0, 255, 136, 0.3);
        }

        /* Tab System */
        .tab-container {
            background: rgba(0, 0, 0, 0.8);
            border: 1px solid #00ff88;
            border-radius: 8px;
            padding: 1.5rem;
            margin-bottom: 2rem;
        }

        .tab-header {
            display: flex;
            border-bottom: 1px solid rgba(0, 255, 136, 0.3);
            margin-bottom: 1.5rem;
        }

        .tab-button {
            background: transparent;
            border: none;
            color: rgba(0, 255, 136, 0.6);
            padding: 0.75rem 1.5rem;
            font-family: 'Courier New', monospace;
            font-size: 0.9rem;
            cursor: pointer;
            border-bottom: 2px solid transparent;
            transition: all 0.3s ease;
            text-transform: uppercase;
            letter-spacing: 1px;
        }

        .tab-button.active {
            color: #00ff88;
            border-bottom-color: #00ff88;
            background: rgba(0, 255, 136, 0.05);
        }

        .tab-button:hover {
            color: #00ff88;
            background: rgba(0, 255, 136, 0.05);
        }

        .tab-content {
            display: none;
        }

        .tab-content.active {
            display: block;
        }

        .api-key-form {
            display: flex;
            flex-direction: column;
            gap: 1rem;
        }

        .form-group {
            display: flex;
            flex-direction: column;
            gap: 0.5rem;
        }

        .form-label {
            color: #00ff88;
            font-size: 0.9rem;
            text-transform: uppercase;
            letter-spacing: 1px;
        }

        .form-input {
            background: transparent;
            border: 1px solid rgba(0, 255, 136, 0.3);
            color: #00ff88;
            padding: 0.75rem;
            border-radius: 4px;
            font-family: 'Courier New', monospace;
            font-size: 0.9rem;
            outline: none;
            transition: all 0.3s ease;
        }

        .form-input:focus {
            border-color: #00ff88;
            box-shadow: 0 0 10px rgba(0, 255, 136, 0.3);
        }

        .form-textarea {
            min-height: 100px;
            resize: vertical;
        }

        .form-button {
            background: transparent;
            border: 1px solid #00ff88;
            color: #00ff88;
            padding: 0.75rem 1.5rem;
            border-radius: 4px;
            font-family: 'Courier New', monospace;
            font-size: 0.9rem;
            cursor: pointer;
            transition: all 0.3s ease;
            text-transform: uppercase;
            letter-spacing: 1px;
        }

        .form-button:hover {
            background: rgba(0, 255, 136, 0.1);
            box-shadow: 0 0 10px rgba(0, 255, 136, 0.3);
        }

        .form-button:disabled {
            opacity: 0.5;
            cursor: not-allowed;
        }

        .test-result {
            margin-top: 1rem;
            padding: 1rem;
            border-radius: 4px;
            border-left: 3px solid #00ff88;
            background: rgba(0, 255, 136, 0.05);
        }

        .test-success {
            border-left-color: #28a745;
            background: rgba(40, 167, 69, 0.05);
            color: #28a745;
        }

        .test-error {
            border-left-color: #dc3545;
            background: rgba(220, 53, 69, 0.05);
            color: #dc3545;
        }

        .api-status {
            display: flex;
            align-items: center;
            gap: 0.5rem;
            padding: 0.5rem 1rem;
            border-radius: 4px;
            font-size: 0.8rem;
            margin-bottom: 1rem;
        }

        .status-configured {
            background: rgba(40, 167, 69, 0.1);
            color: #28a745;
            border: 1px solid rgba(40, 167, 69, 0.3);
        }

        .status-not-configured {
            background: rgba(255, 193, 7, 0.1);
            color: #ffc107;
            border: 1px solid rgba(255, 193, 7, 0.3);
        }

        .status-testing {
            background: rgba(0, 123, 255, 0.1);
            color: #007bff;
            border: 1px solid rgba(0, 123, 255, 0.3);
        }

        .loading-spinner {
            display: inline-block;
            width: 12px;
            height: 12px;
            border: 2px solid rgba(0, 255, 136, 0.3);
            border-radius: 50%;
            border-top-color: #00ff88;
            animation: spin 1s linear infinite;
        }

        /* Notification System */
        .notification-container {
            position: fixed;
            top: 20px;
            right: 20px;
