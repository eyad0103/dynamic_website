        function formatAIResponse(response) {
            // Convert markdown-style formatting to HTML
            let formatted = escapeHtml(response);
            
            // First, handle the color-coded formatting more carefully
            // Look for patterns like "**Bold Blue** text" and convert to colored spans
            
            // Replace **Bold Blue** text patterns
            formatted = formatted.replace(/\*\*Bold Blue\*\* (.*?)(?=\*\*|$)/g, '<span class="blue-bold">$1</span>');
            
            // Replace **Green** text patterns  
            formatted = formatted.replace(/\*\*Green\*\* (.*?)(?=\*\*|$)/g, '<span class="green-bold">$1</span>');
            
            // Replace **Red** text patterns
            formatted = formatted.replace(/\*\*Red\*\* (.*?)(?=\*\*|$)/g, '<span class="red-bold">$1</span>');
            
            // Handle remaining **text** patterns (default to blue)
            formatted = formatted.replace(/\*\*(.*?)\*\*/g, '<span class="blue-bold">$1</span>');
            
            // Convert bullet points
            formatted = formatted.replace(/‚Ä¢ (.*?)(?=\n|$)/g, '<li>$1</li>');
            
            // Wrap consecutive <li> tags in <ul>
            formatted = formatted.replace(/(<li>.*?<\/li>)(\s*<li>.*?<\/li>)+/g, function(match) {
                return '<ul>' + match + '</ul>';
            });
            
            // Convert line breaks to <br>
            formatted = formatted.replace(/\n/g, '<br>');
            
            // Clean up any remaining formatting issues
            formatted = formatted.replace(/\*\*/g, '');
            
            return formatted;
        }
        
