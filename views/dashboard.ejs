<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>AI Error Analysis Dashboard</title>
    <meta name="description" content="AI-powered app error analysis and monitoring dashboard">
    <link rel="stylesheet" href="/css/style.css?v=1.0.1">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
    <style>
        /* Error Dashboard Styles */
        .dashboard-container {
            min-height: 100vh;
            background: linear-gradient(135deg, #0a0a0a 0%, #1a1a2e 100%);
            color: #00ff88;
            font-family: 'Courier New', monospace;
            position: relative;
            overflow: hidden;
        }

        .dashboard-container::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background-image: 
                repeating-linear-gradient(0deg, transparent, transparent 2px, rgba(0, 255, 136, 0.03) 2px, rgba(0, 255, 136, 0.03) 4px),
                repeating-linear-gradient(90deg, transparent, transparent 2px, rgba(0, 255, 136, 0.03) 2px, rgba(0, 255, 136, 0.03) 4px);
            background-size: 50px 50px;
            pointer-events: none;
            z-index: 1;
        }

        .dashboard-header {
            background: rgba(0, 0, 0, 0.9);
            border-bottom: 2px solid #00ff88;
            padding: 1rem 0;
            position: relative;
            z-index: 10;
            backdrop-filter: blur(10px);
        }

        .header-content {
            max-width: 1400px;
            margin: 0 auto;
            padding: 0 2rem;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        .dashboard-title {
            font-size: 1.8rem;
            font-weight: bold;
            text-transform: uppercase;
            letter-spacing: 3px;
            text-shadow: 0 0 15px #00ff88;
            animation: glow 2s ease-in-out infinite alternate;
        }

        @keyframes glow {
            from { text-shadow: 0 0 15px #00ff88; }
            to { text-shadow: 0 0 25px #00ff88, 0 0 35px #00ff88; }
        }

        .status-indicator {
            display: flex;
            align-items: center;
            gap: 0.5rem;
            padding: 0.5rem 1rem;
            background: rgba(0, 255, 136, 0.1);
            border: 1px solid #00ff88;
            border-radius: 4px;
            font-size: 0.9rem;
        }

        .status-dot {
            width: 8px;
            height: 8px;
            background: #00ff88;
            border-radius: 50%;
            animation: pulse 2s infinite;
        }

        @keyframes pulse {
            0%, 100% { opacity: 1; }
            50% { opacity: 0.5; }
        }

        .error-stats {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
            gap: 1.5rem;
            margin-bottom: 2rem;
        }

        .stat-card {
            background: rgba(0, 20, 40, 0.8);
            border: 1px solid rgba(0, 255, 136, 0.3);
            border-radius: 8px;
            padding: 1.5rem;
            position: relative;
            backdrop-filter: blur(5px);
            transition: all 0.3s ease;
        }

        .stat-card:hover {
            border-color: #00ff88;
            box-shadow: 0 0 20px rgba(0, 255, 136, 0.3);
            transform: translateY(-2px);
        }

        .stat-label {
            font-size: 0.8rem;
            color: #00ff88;
            text-transform: uppercase;
            letter-spacing: 1px;
            margin-bottom: 0.5rem;
            opacity: 0.8;
        }

        .stat-value {
            font-size: 1.8rem;
            font-weight: bold;
            color: #ffffff;
            text-shadow: 0 0 5px rgba(0, 255, 136, 0.5);
        }

        .error-list {
            background: rgba(0, 0, 0, 0.8);
            border: 1px solid #00ff88;
            border-radius: 8px;
            padding: 1.5rem;
            margin-bottom: 2rem;
        }

        .error-list-header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            margin-bottom: 1.5rem;
            padding-bottom: 1rem;
            border-bottom: 1px solid rgba(0, 255, 136, 0.3);
        }

        .error-list-title {
            font-size: 1.2rem;
            color: #00ff88;
            text-transform: uppercase;
            letter-spacing: 2px;
        }

        .error-filters {
            display: flex;
            gap: 1rem;
            align-items: center;
        }

        .filter-input {
            background: transparent;
            border: 1px solid rgba(0, 255, 136, 0.3);
            color: #00ff88;
            padding: 0.5rem;
            border-radius: 4px;
            font-family: 'Courier New', monospace;
            font-size: 0.9rem;
            outline: none;
        }

        .filter-input:focus {
            border-color: #00ff88;
            box-shadow: 0 0 10px rgba(0, 255, 136, 0.3);
        }

        .error-item {
            background: rgba(0, 255, 136, 0.05);
            border: 1px solid rgba(0, 255, 136, 0.2);
            border-radius: 4px;
            padding: 1rem;
            margin-bottom: 1rem;
            transition: all 0.3s ease;
            cursor: pointer;
        }

        .error-item:hover {
            background: rgba(0, 255, 136, 0.1);
            border-color: #00ff88;
            transform: translateX(5px);
        }

        .error-header {
            display: flex;
            justify-content: space-between;
            align-items: flex-start;
            margin-bottom: 0.5rem;
        }

        .error-app-name {
            font-size: 1.1rem;
            color: #ffffff;
            font-weight: bold;
        }

        .error-timestamp {
            font-size: 0.8rem;
            color: rgba(0, 255, 136, 0.6);
        }

        .error-status {
            display: inline-block;
            padding: 0.25rem 0.5rem;
            border-radius: 4px;
            font-size: 0.7rem;
            text-transform: uppercase;
            letter-spacing: 1px;
            margin-bottom: 0.5rem;
        }

        .status-pending {
            background: rgba(255, 193, 7, 0.2);
            color: #ffc107;
            border: 1px solid rgba(255, 193, 7, 0.3);
        }

        .status-analyzed {
            background: rgba(40, 167, 69, 0.2);
            color: #28a745;
            border: 1px solid rgba(40, 167, 69, 0.3);
        }

        .status-failed {
            background: rgba(220, 53, 69, 0.2);
            color: #dc3545;
            border: 1px solid rgba(220, 53, 69, 0.3);
        }

        .error-message {
            color: rgba(255, 255, 255, 0.8);
            font-size: 0.9rem;
            margin-bottom: 0.5rem;
            font-family: 'Courier New', monospace;
            background: rgba(0, 0, 0, 0.3);
            padding: 0.5rem;
            border-radius: 4px;
            border-left: 3px solid #00ff88;
        }

        .error-ai-explanation {
            color: #00ff88;
            font-size: 0.9rem;
            line-height: 1.4;
            background: rgba(0, 255, 136, 0.05);
            padding: 0.5rem;
            border-radius: 4px;
            border-left: 3px solid #00ff88;
            margin-top: 0.5rem;
        }

        .no-errors {
            text-align: center;
            color: rgba(0, 255, 136, 0.6);
            font-size: 1.1rem;
            padding: 2rem;
        }

        .loading {
            text-align: center;
            color: #00ff88;
            font-size: 1.1rem;
            padding: 2rem;
        }

        .refresh-btn {
            background: transparent;
            border: 1px solid #00ff88;
            color: #00ff88;
            padding: 0.5rem 1rem;
            border-radius: 4px;
            font-family: 'Courier New', monospace;
            cursor: pointer;
            transition: all 0.3s ease;
        }

        .refresh-btn:hover {
            background: rgba(0, 255, 136, 0.1);
            box-shadow: 0 0 10px rgba(0, 255, 136, 0.3);
        }

        /* Tab System */
        .tab-container {
            background: rgba(0, 0, 0, 0.8);
            border: 1px solid #00ff88;
            border-radius: 8px;
            padding: 1.5rem;
            margin-bottom: 2rem;
        }

        .tab-header {
            display: flex;
            border-bottom: 1px solid rgba(0, 255, 136, 0.3);
            margin-bottom: 1.5rem;
        }

        .tab-button {
            background: transparent;
            border: none;
            color: rgba(0, 255, 136, 0.6);
            padding: 0.75rem 1.5rem;
            font-family: 'Courier New', monospace;
            font-size: 0.9rem;
            cursor: pointer;
            border-bottom: 2px solid transparent;
            transition: all 0.3s ease;
            text-transform: uppercase;
            letter-spacing: 1px;
        }

        .tab-button.active {
            color: #00ff88;
            border-bottom-color: #00ff88;
            background: rgba(0, 255, 136, 0.05);
        }

        .tab-button:hover {
            color: #00ff88;
            background: rgba(0, 255, 136, 0.05);
        }

        .tab-content {
            display: none;
        }

        .tab-content.active {
            display: block;
        }

        .api-key-form {
            display: flex;
            flex-direction: column;
            gap: 1rem;
        }

        .form-group {
            display: flex;
            flex-direction: column;
            gap: 0.5rem;
        }

        .form-label {
            color: #00ff88;
            font-size: 0.9rem;
            text-transform: uppercase;
            letter-spacing: 1px;
        }

        .form-input {
            background: transparent;
            border: 1px solid rgba(0, 255, 136, 0.3);
            color: #00ff88;
            padding: 0.75rem;
            border-radius: 4px;
            font-family: 'Courier New', monospace;
            font-size: 0.9rem;
            outline: none;
            transition: all 0.3s ease;
        }

        .form-input:focus {
            border-color: #00ff88;
            box-shadow: 0 0 10px rgba(0, 255, 136, 0.3);
        }

        .form-textarea {
            min-height: 100px;
            resize: vertical;
        }

        .form-button {
            background: transparent;
            border: 1px solid #00ff88;
            color: #00ff88;
            padding: 0.75rem 1.5rem;
            border-radius: 4px;
            font-family: 'Courier New', monospace;
            font-size: 0.9rem;
            cursor: pointer;
            transition: all 0.3s ease;
            text-transform: uppercase;
            letter-spacing: 1px;
        }

        .form-button:hover {
            background: rgba(0, 255, 136, 0.1);
            box-shadow: 0 0 10px rgba(0, 255, 136, 0.3);
        }

        .form-button:disabled {
            opacity: 0.5;
            cursor: not-allowed;
        }

        .test-result {
            margin-top: 1rem;
            padding: 1rem;
            border-radius: 4px;
            border-left: 3px solid #00ff88;
            background: rgba(0, 255, 136, 0.05);
        }

        .test-success {
            border-left-color: #28a745;
            background: rgba(40, 167, 69, 0.05);
            color: #28a745;
        }

        .test-error {
            border-left-color: #dc3545;
            background: rgba(220, 53, 69, 0.05);
            color: #dc3545;
        }

        .api-status {
            display: flex;
            align-items: center;
            gap: 0.5rem;
            padding: 0.5rem 1rem;
            border-radius: 4px;
            font-size: 0.8rem;
            margin-bottom: 1rem;
        }

        .status-configured {
            background: rgba(40, 167, 69, 0.1);
            color: #28a745;
            border: 1px solid rgba(40, 167, 69, 0.3);
        }

        .status-not-configured {
            background: rgba(255, 193, 7, 0.1);
            color: #ffc107;
            border: 1px solid rgba(255, 193, 7, 0.3);
        }

        .status-testing {
            background: rgba(0, 123, 255, 0.1);
            color: #007bff;
            border: 1px solid rgba(0, 123, 255, 0.3);
        }

        .loading-spinner {
            display: inline-block;
            width: 12px;
            height: 12px;
            border: 2px solid rgba(0, 255, 136, 0.3);
            border-radius: 50%;
            border-top-color: #00ff88;
            animation: spin 1s linear infinite;
        }

        /* Notification System */
        .notification-container {
            position: fixed;
            top: 20px;
            right: 20px;
            z-index: 1000;
            max-width: 300px;
        }

        .notification {
            background: rgba(0, 0, 0, 0.9);
            border: 1px solid #00ff88;
            border-radius: 4px;
            padding: 1rem;
            margin-bottom: 1rem;
            backdrop-filter: blur(10px);
            animation: slideIn 0.3s ease;
            box-shadow: 0 0 20px rgba(0, 255, 136, 0.3);
        }

        @keyframes slideIn {
            from {
                transform: translateX(100%);
                opacity: 0;
            }
            to {
                transform: translateX(0);
                opacity: 1;
            }
        }

        .notification-header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            margin-bottom: 0.5rem;
            padding-bottom: 0.5rem;
            border-bottom: 1px solid rgba(0, 255, 136, 0.3);
        }

        .notification-title {
            color: #00ff88;
            font-size: 0.9rem;
            text-transform: uppercase;
            letter-spacing: 1px;
        }

        .notification-close {
            background: transparent;
            border: none;
            color: #00ff88;
            font-size: 1.2rem;
            cursor: pointer;
            opacity: 0.7;
            transition: opacity 0.3s ease;
        }

        .notification-close:hover {
            opacity: 1;
        }

        .notification-body {
            color: rgba(255, 255, 255, 0.9);
            font-size: 0.9rem;
            line-height: 1.4;
        }

        .notification-success {
            border-color: #28a745;
            box-shadow: 0 0 20px rgba(40, 167, 69, 0.3);
        }

        .notification-success .notification-title {
            color: #28a745;
        }

        .notification-error {
            border-color: #dc3545;
            box-shadow: 0 0 20px rgba(220, 53, 69, 0.3);
        }

        .notification-error .notification-title {
            color: #dc3545;
        }

        .notification-info {
            border-color: #007bff;
            box-shadow: 0 0 20px rgba(0, 123, 255, 0.3);
        }

        .notification-info .notification-title {
            color: #007bff;
        }

        .version-badge {
            position: fixed;
            top: 20px;
            left: 20px;
            background: rgba(0, 0, 0, 0.9);
            border: 1px solid #00ff88;
            border-radius: 4px;
            padding: 0.5rem 1rem;
            color: #00ff88;
            font-family: 'Courier New', monospace;
            font-size: 0.8rem;
            z-index: 1000;
            backdrop-filter: blur(10px);
        }

        .loading-overlay {
            position: fixed;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: rgba(0, 0, 0, 0.8);
            display: none;
            justify-content: center;
            align-items: center;
            z-index: 9999;
        }

        .loading-spinner {
            width: 50px;
            height: 50px;
            border: 3px solid rgba(0, 255, 136, 0.3);
            border-top-color: #00ff88;
            border-radius: 50%;
            animation: spin 1s linear infinite;
        }

        @keyframes spin {
            to { transform: rotate(360deg); }
        }

        /* Responsive Design */
        @media (max-width: 768px) {
            .header-content {
                flex-direction: column;
                gap: 1rem;
                text-align: center;
            }

            .dashboard-title {
                font-size: 1.5rem;
            }

            .error-stats {
                grid-template-columns: 1fr;
            }

            .error-filters {
                flex-direction: column;
                width: 100%;
            }

            .filter-input {
                width: 100%;
            }

            .error-header {
                flex-direction: column;
                gap: 0.5rem;
            }
        }
    </style>
</head>
<body>
    <div class="dashboard-container">
        <!-- Version Badge -->
        <div class="version-badge" id="versionBadge">
            v2.0.1
        </div>
        
        <!-- Notification Container -->
        <div class="notification-container" id="notificationContainer"></div>
        
        <!-- Loading Overlay -->
        <div class="loading-overlay" id="loadingOverlay">
            <div class="loading-spinner"></div>
        </div>
        <header class="dashboard-header">
            <div class="header-content">
                <h1 class="dashboard-title">
                    <i class="fas fa-brain"></i> AI Error Analysis Dashboard
                </h1>
                <div class="status-indicator">
                    <div class="status-dot"></div>
                    <span>MONITORING</span>
                </div>
            </div>
        </header>

        <main class="dashboard-content">
            <!-- Error Statistics -->
            <div class="error-stats">
                <div class="stat-card">
                    <div class="stat-label">Total Errors</div>
                    <div class="stat-value" id="totalErrors">0</div>
                </div>
                <div class="stat-card">
                    <div class="stat-label">Pending Analysis</div>
                    <div class="stat-value" id="pendingErrors">0</div>
                </div>
                <div class="stat-card">
                    <div class="stat-label">Analyzed</div>
                    <div class="stat-value" id="analyzedErrors">0</div>
                </div>
                <div class="stat-card">
                    <div class="stat-label">Failed Analysis</div>
                    <div class="stat-value" id="failedErrors">0</div>
                </div>
            </div>

            <!-- Tab Container -->
            <div class="tab-container">
                <div class="tab-header">
                    <button class="tab-button active" onclick="switchTab('errors', event)">
                        <i class="fas fa-exclamation-triangle"></i> Errors
                    </button>
                    <button class="tab-button" onclick="switchTab('api', event)">
                        <i class="fas fa-key"></i> API Keys
                    </button>
                    <button class="tab-button" onclick="switchTab('test', event)">
                        <i class="fas fa-flask"></i> Test
                    </button>
                </div>

                <!-- Errors Tab -->
                <div id="errors-tab" class="tab-content active">
                    <div class="error-list">
                        <div class="error-list-header">
                            <div class="error-list-title">Recent Errors</div>
                            <div class="error-filters">
                                <input type="text" id="searchInput" class="filter-input" placeholder="Search errors...">
                                <button id="refreshBtn" class="refresh-btn">
                                    <i class="fas fa-sync-alt"></i> Refresh
                                </button>
                            </div>
                        </div>
                        <div id="errorContainer">
                            <div class="loading">Loading errors...</div>
                        </div>
                    </div>
                </div>

                <!-- API Keys Tab -->
                <div id="api-tab" class="tab-content">
                    <div class="api-status" id="apiStatus">
                        <i class="fas fa-info-circle"></i>
                        <span id="apiStatusText">Checking API configuration...</span>
                    </div>
                    
                    <form class="api-key-form" id="apiKeyForm">
                        <div class="form-group">
                            <label class="form-label" for="apiKey">OpenRouter API Key</label>
                            <input type="password" id="apiKey" class="form-input" placeholder="sk-or-v1-..." value="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">
                            <small style="color: rgba(0, 255, 136, 0.6); font-size: 0.8rem;">Your API key is stored securely and never exposed to the client</small>
                        </div>

                        <div class="form-group">
                            <label class="form-label" for="testPrompt">Test Prompt</label>
                            <textarea id="testPrompt" class="form-input form-textarea" placeholder="Enter a test prompt to verify API connectivity...">Hello, can you confirm this API key is working correctly?</textarea>
                        </div>

                        <div style="display: flex; gap: 1rem;">
                            <button type="button" class="form-button" onclick="testApiKey()">
                                <i class="fas fa-flask"></i> Test API
                            </button>
                            <button type="button" class="form-button" onclick="saveApiKey()">
                                <i class="fas fa-save"></i> Save Key
                            </button>
                        </div>

                        <div id="testResult" style="display: none;"></div>
                    </form>

                    <div style="margin-top: 2rem; padding: 1rem; background: rgba(0, 255, 136, 0.05); border-radius: 4px; border-left: 3px solid #00ff88;">
                        <h4 style="color: #00ff88; margin-bottom: 0.5rem;">API Configuration Instructions</h4>
                        <p style="color: rgba(255, 255, 255, 0.8); font-size: 0.9rem; line-height: 1.4;">
                            1. Enter your OpenRouter API key above<br>
                            2. Click "Test API" to verify connectivity<br>
                            3. Click "Save Key" to store it securely<br>
                            4. The system will automatically use this key for error analysis
                        </p>
                    </div>
                </div>

                <!-- Test Tab -->
                <div id="test-tab" class="tab-content">
                    <div class="error-list">
                        <div class="error-list-header">
                            <div class="error-list-title">System Testing</div>
                            <div class="error-filters">
                                <button id="runAllTestsBtn" class="refresh-btn">
                                    <i class="fas fa-play"></i> Run All Tests
                                </button>
                            </div>
                        </div>
                        
                        <div style="padding: 1rem;">
                            <h3 style="color: #00ff88; margin-bottom: 1rem;">Available Tests</h3>
                            
                            <div style="display: grid; gap: 1rem;">
                                <div class="error-item">
                                    <div class="error-header">
                                        <div class="error-app-name">API Connection Test</div>
                                        <div class="error-timestamp">Ready</div>
                                    </div>
                                    <div class="error-status status-pending">PENDING</div>
                                    <div class="error-message">Test all API endpoints for connectivity and response times</div>
                                    <button class="form-button" onclick="runApiTest()" style="margin-top: 0.5rem;">
                                        <i class="fas fa-plug"></i> Test APIs
                                    </button>
                                </div>
                                
                                <div class="error-item">
                                    <div class="error-header">
                                        <div class="error-app-name">Error Analysis Test</div>
                                        <div class="error-timestamp">Ready</div>
                                    </div>
                                    <div class="error-status status-pending">PENDING</div>
                                    <div class="error-message">Test AI error analysis with sample error data</div>
                                    <button class="form-button" onclick="runErrorAnalysisTest()" style="margin-top: 0.5rem;">
                                        <i class="fas fa-brain"></i> Test Analysis
                                    </button>
                                </div>
                                
                                <div class="error-item">
                                    <div class="error-header">
                                        <div class="error-app-name">Notification Test</div>
                                        <div class="error-timestamp">Ready</div>
                                    </div>
                                    <div class="error-status status-pending">PENDING</div>
                                    <div class="error-message">Test notification system with different message types</div>
                                    <button class="form-button" onclick="runNotificationTest()" style="margin-top: 0.5rem;">
                                        <i class="fas fa-bell"></i> Test Notifications
                                    </button>
                                </div>
                                
                                <div class="error-item">
                                    <div class="error-header">
                                        <div class="error-app-name">Database Test</div>
                                        <div class="error-timestamp">Ready</div>
                                    </div>
                                    <div class="error-status status-pending">PENDING</div>
                                    <div class="error-message">Test error storage and retrieval functionality</div>
                                    <button class="form-button" onclick="runDatabaseTest()" style="margin-top: 0.5rem;">
                                        <i class="fas fa-database"></i> Test Database
                                    </button>
                                </div>
                            </div>
                            
                            <div id="testResults" style="margin-top: 2rem; padding: 1rem; background: rgba(0, 255, 136, 0.05); border-radius: 4px; border-left: 3px solid #00ff88;">
                                <h4 style="color: #00ff88; margin-bottom: 0.5rem;">Test Results</h4>
                                <div id="testResultsContent" style="color: rgba(255, 255, 255, 0.8); font-size: 0.9rem;">
                                    Click "Run All Tests" to execute all system tests...
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </main>
    </div>

    <script>
        // AI Error Analysis Dashboard with API Key Management and Notifications
        let errors = [];
        let filteredErrors = [];
        let currentVersion = 'v2.0.3';
        
            // Update version badge
            updateVersionBadge();
            
            const errorContainer = document.getElementById('errorContainer');
            const searchInput = document.getElementById('searchInput');
            const refreshBtn = document.getElementById('refreshBtn');
            const totalErrorsEl = document.getElementById('totalErrors');
            const pendingErrorsEl = document.getElementById('pendingErrors');
            const analyzedErrorsEl = document.getElementById('analyzedErrors');
            const failedErrorsEl = document.getElementById('failedErrors');
            
            // Load initial errors
            loadErrors();
            
            // Check API status
            checkApiStatus();
            
            // Auto-refresh every 30 seconds
            setInterval(loadErrors, 30000);
            
            // Search functionality
            searchInput.addEventListener('input', function() {
                const searchTerm = this.value.toLowerCase();
                filteredErrors = errors.filter(error => 
                    error.appName.toLowerCase().includes(searchTerm) ||
                    error.error.toLowerCase().includes(searchTerm) ||
                    error.userInfo.toLowerCase().includes(searchTerm)
                );
                renderErrors();
            });
            
            // Refresh button
            refreshBtn.addEventListener('click', function() {
                this.disabled = true;
                this.innerHTML = '<i class="fas fa-sync-alt fa-spin"></i> Refreshing...';
                loadErrors().finally(() => {
                    this.disabled = false;
                    this.innerHTML = '<i class="fas fa-sync-alt"></i> Refresh';
                });
            });
        });
        
        // Notification System
        function showNotification(type, title, message) {
            console.log('üîî Notification:', type, title, message);
            
            const container = document.getElementById('notificationContainer');
            if (!container) {
                console.error('Notification container not found');
                return;
            }
            
            const notification = document.createElement('div');
            notification.className = `notification notification-${type}`;
            
            notification.innerHTML = `
                <div class="notification-header">
                    <div class="notification-title">${title}</div>
                    <button class="notification-close" onclick="this.parentElement.remove()">√ó</button>
                </div>
                <div class="notification-body">${message}</div>
            `;
            
            container.appendChild(notification);
            
            console.log('üîî Notification added to container');
            
            // Auto-remove after 5 seconds
            setTimeout(() => {
                if (notification.parentElement) {
                    console.log('üîî Removing notification');
                    notification.remove();
                }
            }, 5000);
        }
        
        function updateVersionBadge() {
            const badge = document.getElementById('versionBadge');
            if (badge) {
                badge.textContent = currentVersion;
                console.log('üè∑Ô∏è Version updated to:', currentVersion);
            } else {
                console.log('‚ùå Version badge not found');
            }
        }
        
        // Test notification system
        function testNotifications() {
            console.log('üß™ Testing notification system...');
            
            // Test different notification types
            setTimeout(() => {
                showNotification('info', 'Test Info', 'This is an info notification');
            }, 1000);
            
            setTimeout(() => {
                showNotification('success', 'Test Success', 'This is a success notification');
            }, 2000);
            
            setTimeout(() => {
                showNotification('warning', 'Test Warning', 'This is a warning notification');
            }, 3000);
            
            setTimeout(() => {
                showNotification('error', 'Test Error', 'This is an error notification');
            }, 4000);
        }
        
        // Test tab switching
        function testTabSwitching() {
            console.log('üîÑ Testing tab switching...');
            
            setTimeout(() => {
                switchTab('errors', { target: document.querySelector('.tab-button:nth-child(2)') });
            }, 1000);
            
            setTimeout(() => {
                switchTab('api', { target: document.querySelector('.tab-button:last-child') });
            }, 2000);
        }
        
        // Auto-test on load
        setTimeout(() => {
            testNotifications();
            testTabSwitching();
        }, 2000);
        
        // Tab switching with better error handling
        function switchTab(tabName, event) {
            try {
                console.log('üîÑ Switching to tab:', tabName);
                
                // Hide all tabs
                document.querySelectorAll('.tab-content').forEach(tab => {
                    tab.classList.remove('active');
                });
                
                // Remove active class from all buttons
                document.querySelectorAll('.tab-button').forEach(button => {
                    button.classList.remove('active');
                });
                
                // Show selected tab
                const targetTab = document.getElementById(tabName + '-tab');
                if (targetTab) {
                    targetTab.classList.add('active');
                    console.log('‚úÖ Tab found and activated:', tabName);
                } else {
                    console.error(`‚ùå Tab "${tabName}" not found`);
                    showNotification('error', 'Tab Error', `Could not find tab: ${tabName}`);
                    return;
                }
                
                // Add active class to clicked button
                if (event && event.target) {
                    event.target.classList.add('active');
                }
                
                // Show success notification
                showNotification('info', 'Tab Switched', `Switched to ${tabName} tab`);
                
                // Check API status when switching to API tab
                if (tabName === 'api') {
                    checkApiStatus();
                }
                
            } catch (error) {
                console.error('‚ùå Tab switching error:', error);
                showNotification('error', 'Tab Error', `Failed to switch to ${tabName}: ${error.message}`);
            }
        }
        
        // API Key Management
        async function checkApiStatus() {
            const apiStatus = document.getElementById('apiStatus');
            const apiStatusText = document.getElementById('apiStatusText');
            
            try {
                const response = await fetch('/api/api-key-status');
                const data = await response.json();
                
                if (data.success) {
                    if (data.configured) {
                        apiStatus.className = 'api-status status-configured';
                        apiStatusText.innerHTML = '<i class="fas fa-check-circle"></i> API key configured and working';
                        
                        // Show masked key
                        if (data.maskedKey) {
                            const maskedKeyDiv = document.createElement('div');
                            maskedKeyDiv.style.cssText = 'color: rgba(0, 255, 136, 0.6); font-size: 0.8rem; margin-top: 0.5rem;';
                            maskedKeyDiv.textContent = 'Key: ' + data.maskedKey;
                            apiStatus.appendChild(maskedKeyDiv);
                        }
                        
                        showNotification('success', 'API Key Active', 'OpenRouter API key is configured and ready for error analysis');
                    } else {
                        apiStatus.className = 'api-status status-not-configured';
                        apiStatusText.innerHTML = '<i class="fas fa-exclamation-triangle"></i> API key not configured';
                        showNotification('warning', 'API Key Required', 'Please configure your OpenRouter API key to enable AI error analysis');
                    }
                } else {
                    throw new Error(data.error || 'Failed to check API status');
                }
                
            } catch (error) {
                console.error('API status check failed:', error);
                apiStatus.className = 'api-status status-not-configured';
                apiStatusText.innerHTML = '<i class="fas fa-times-circle"></i> Unable to check API status';
                showNotification('error', 'Connection Error', 'Failed to check API status: ' + error.message);
            }
        }
        
        async function testApiKey() {
            const apiKey = document.getElementById('apiKey').value;
            const testPrompt = document.getElementById('testPrompt').value;
            const testResult = document.getElementById('testResult');
            const apiStatus = document.getElementById('apiStatus');
            const apiStatusText = document.getElementById('apiStatusText');
            
            if (!apiKey) {
                showTestResult('Please enter an API key', 'error');
                return;
            }
            
            if (!testPrompt) {
                showTestResult('Please enter a test prompt', 'error');
                return;
            }
            
            // Update status
            apiStatus.className = 'api-status status-testing';
            apiStatusText.innerHTML = '<span class="loading-spinner"></span> Testing API connection...';
            
            showTestResult('Testing API connection...', 'loading');
            
            try {
                const response = await fetch('/api/test-api-key', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({
                        apiKey: apiKey,
                        testPrompt: testPrompt
                    })
                });
                
                const data = await response.json();
                
                if (data.success) {
                    showTestResult('API connection successful! Response: ' + data.response, 'success');
                    apiStatus.className = 'api-status status-configured';
                    apiStatusText.textContent = 'API key configured and working';
                } else {
                    showTestResult('API test failed: ' + data.error, 'error');
                    apiStatus.className = 'api-status status-not-configured';
                    apiStatusText.textContent = 'API key test failed';
                }
                
            } catch (error) {
                showTestResult('API test failed: ' + error.message, 'error');
                apiStatus.className = 'api-status status-not-configured';
                apiStatusText.textContent = 'API key test failed';
            }
        }
        
        async function saveApiKey() {
            const apiKey = document.getElementById('apiKey').value;
            const apiStatus = document.getElementById('apiStatus');
            const apiStatusText = document.getElementById('apiStatusText');
            
            if (!apiKey) {
                showTestResult('Please enter an API key', 'error');
                return;
            }
            
            apiStatus.className = 'api-status status-testing';
            apiStatusText.innerHTML = '<span class="loading-spinner"></span> Saving API key...';
            
            try {
                const response = await fetch('/api/save-api-key', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({
                        apiKey: apiKey
                    })
                });
                
                const data = await response.json();
                
                if (data.success) {
                    showTestResult('API key saved successfully!', 'success');
                    apiStatus.className = 'api-status status-configured';
                    apiStatusText.textContent = 'API key configured and saved';
                } else {
                    showTestResult('Failed to save API key: ' + data.error, 'error');
                    apiStatus.className = 'api-status status-not-configured';
                    apiStatusText.textContent = 'Failed to save API key';
                }
                
            } catch (error) {
                showTestResult('Failed to save API key: ' + error.message, 'error');
                apiStatus.className = 'api-status status-not-configured';
                apiStatusText.textContent = 'Failed to save API key';
            }
        }
        
        function showTestResult(message, type) {
            const testResult = document.getElementById('testResult');
            testResult.style.display = 'block';
            testResult.className = 'test-result test-' + type;
            testResult.textContent = message;
            }
            
            function loadErrors() {
                return fetch('/api/errors')
                    .then(response => response.json())
                    .then(data => {
                        if (data.success) {
                            errors = data.errors;
                            filteredErrors = errors;
                            renderErrors();
                            updateStats();
                        } else {
                            throw new Error(data.error || 'Failed to load errors');
                        }
                    })
                    .catch(error => {
                        console.error('Error loading errors:', error);
                        errorContainer.innerHTML = '<div class="no-errors">Failed to load errors. Please try again.</div>';
                    });
            }
            
            function renderErrors() {
                if (filteredErrors.length === 0) {
                    errorContainer.innerHTML = '<div class="no-errors">No errors found.</div>';
                    return;
                }
                
                errorContainer.innerHTML = filteredErrors.map(error => `
                    <div class="error-item" data-id="${error.id}">
                        <div class="error-header">
                            <div class="error-app-name">${escapeHtml(error.appName)}</div>
                            <div class="error-timestamp">${formatDate(error.timestamp)}</div>
                        </div>
                        <div class="error-status status-${error.status}">${getStatusText(error.status)}</div>
                        <div class="error-message">${escapeHtml(error.error)}</div>
                        ${error.aiExplanation ? `<div class="error-ai-explanation">${escapeHtml(error.aiExplanation)}</div>` : ''}
                        ${error.userInfo ? `<div style="color: rgba(0, 255, 136, 0.6); font-size: 0.8rem; margin-top: 0.5rem;">User: ${escapeHtml(error.userInfo)}</div>` : ''}
                    </div>
                `).join('');
            }
            
            function updateStats() {
                const total = errors.length;
                const pending = errors.filter(e => e.status === 'pending_analysis').length;
                const analyzed = errors.filter(e => e.status === 'analyzed').length;
                const failed = errors.filter(e => e.status === 'analysis_failed').length;
                
                totalErrorsEl.textContent = total;
                pendingErrorsEl.textContent = pending;
                analyzedErrorsEl.textContent = analyzed;
                failedErrorsEl.textContent = failed;
            }
            
            function formatDate(timestamp) {
                const date = new Date(timestamp);
                return date.toLocaleString();
            }
            
            function getStatusText(status) {
                switch (status) {
                    case 'pending_analysis': return 'Pending Analysis';
                    case 'analyzed': return 'Analyzed';
                    case 'analysis_failed': return 'Analysis Failed';
                    default: return 'Unknown';
                }
            }
            
            function escapeHtml(text) {
                const div = document.createElement('div');
                div.textContent = text;
                return div.innerHTML;
            }
            
            try {
                // Create a test error
                const testError = {
                    appName: 'TestApp',
                    error: 'TypeError: Cannot read property of undefined',
                    userInfo: 'TestUser'
                };
                
                // Send test error for analysis
                const response = await fetch('/api/errors/capture', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify(testError)
                });
                
                const result = await response.json();
                
                if (result.success) {
                    resultsDiv.innerHTML = `
                        <div style="margin-bottom: 1rem;">
                            <strong>‚úÖ Error Capture:</strong> Success<br>
                            <strong>üìù Error ID:</strong> ${result.errorId}<br>
                            <strong>üîç Status:</strong> Queued for AI analysis
                        </div>
                    `;
                    showNotification('success', 'Analysis Test Complete', 'Test error captured successfully');
                } else {
                    throw new Error(result.error);
                }
                
            } catch (error) {
                resultsDiv.innerHTML = `<strong>‚ùå Analysis Test Failed:</strong> ${error.message}`;
                showNotification('error', 'Analysis Test Failed', error.message);
            }
        }
        
        function runNotificationTest() {
            showNotification('info', 'Notification Test', 'Testing notification system...');
            const resultsDiv = document.getElementById('testResultsContent');
            
            // Test different notification types
            setTimeout(() => showNotification('success', 'Success Test', 'This is a success notification'), 500);
            setTimeout(() => showNotification('warning', 'Warning Test', 'This is a warning notification'), 1000);
            setTimeout(() => showNotification('error', 'Error Test', 'This is an error notification'), 1500);
            
            resultsDiv.innerHTML = `
                <div style="margin-bottom: 1rem;">
                    <strong>‚úÖ Notification System:</strong> Working<br>
                    <strong>üì¢ Test Messages:</strong> 4 notifications sent<br>
                    <strong>üé® Types Tested:</strong> Info, Success, Warning, Error
                </div>
            `;
            
            setTimeout(() => {
                showNotification('success', 'Notification Test Complete', 'All notification types working correctly');
            }, 2000);
        }
        
        async function runDatabaseTest() {
            showNotification('info', 'Database Test', 'Testing error storage...');
            const resultsDiv = document.getElementById('testResultsContent');
            
            try {
                // Test retrieving errors
                const response = await fetch('/api/errors');
                const data = await response.json();
                
                if (data.success) {
                    resultsDiv.innerHTML = `
                        <div style="margin-bottom: 1rem;">
                            <strong>‚úÖ Error Retrieval:</strong> Working<br>
                            <strong>üìä Total Errors:</strong> ${data.errors.length}<br>
                            <strong>üîÑ API Response:</strong> ${response.status} ${response.statusText}
                        </div>
                    `;
                    showNotification('success', 'Database Test Complete', 'Error storage and retrieval working');
                } else {
                    throw new Error(data.error);
                }
                
            } catch (error) {
                resultsDiv.innerHTML = `<strong>‚ùå Database Test Failed:</strong> ${error.message}`;
                showNotification('error', 'Database Test Failed', error.message);
            }
        }
        
        // Run all tests
        async function runAllTests() {
            showNotification('info', 'All Tests', 'Running all system tests...');
            const resultsDiv = document.getElementById('testResultsContent');
            const runAllBtn = document.getElementById('runAllTestsBtn');
            
            // Disable button during tests
            runAllBtn.disabled = true;
            runAllBtn.innerHTML = '<i class="fas fa-spinner fa-spin"></i> Running...';
            
            try {
                await runApiTest();
                await new Promise(resolve => setTimeout(resolve, 1000));
                await runErrorAnalysisTest();
                await new Promise(resolve => setTimeout(resolve, 1000));
                runNotificationTest();
                await new Promise(resolve => setTimeout(resolve, 2500));
                await runDatabaseTest();
                
                resultsDiv.innerHTML = `
                    <div style="margin-bottom: 1rem; color: #28a745;">
                        <strong>üéâ All Tests Complete!</strong><br>
                        ‚úÖ API Connection Test: Passed<br>
                        ‚úÖ Error Analysis Test: Passed<br>
                        ‚úÖ Notification Test: Passed<br>
                        ‚úÖ Database Test: Passed
                    </div>
                `;
                
                showNotification('success', 'All Tests Complete', 'All system tests passed successfully');
                
            } catch (error) {
                showNotification('error', 'Tests Failed', 'Some tests failed: ' + error.message);
            } finally {
                // Re-enable button
                runAllBtn.disabled = false;
                runAllBtn.innerHTML = '<i class="fas fa-play"></i> Run All Tests';
            }
        }
        
        // Add event listener for run all tests button
        document.addEventListener('DOMContentLoaded', function() {
            const runAllBtn = document.getElementById('runAllTestsBtn');
            if (runAllBtn) {
                runAllBtn.addEventListener('click', runAllTests);
            }
        });
    </script>
</body>
</html>
