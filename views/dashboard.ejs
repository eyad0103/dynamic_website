                const response = await fetch('/api/test-api-key', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({
                        apiKey: apiKey,
                        testPrompt: testPrompt
                    })
                });
                
                const data = await response.json();
                
                if (data.success) {
                    showTestResult('API connection successful! Response: ' + data.response, 'success');
                    apiStatus.className = 'api-status status-configured';
                    apiStatusText.textContent = 'API key configured and working';
                } else {
                    showTestResult('API test failed: ' + data.error, 'error');
                    apiStatus.className = 'api-status status-not-configured';
                    apiStatusText.textContent = 'API key test failed';
                }
                
            } catch (error) {
                showTestResult('API test failed: ' + error.message, 'error');
                apiStatus.className = 'api-status status-not-configured';
                apiStatusText.textContent = 'API key test failed';
            }
        }
        
        async function saveApiKey() {
            const apiKey = document.getElementById('apiKey').value;
            const apiStatus = document.getElementById('apiStatus');
            const apiStatusText = document.getElementById('apiStatusText');
            
            if (!apiKey) {
                showTestResult('Please enter an API key', 'error');
                return;
            }
            
            apiStatus.className = 'api-status status-testing';
            apiStatusText.innerHTML = '<span class="loading-spinner"></span> Saving API key...';
            
            try {
                const response = await fetch('/api/save-api-key', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({
                        apiKey: apiKey
                    })
                });
                
                const data = await response.json();
                
                if (data.success) {
                    showTestResult('API key saved successfully!', 'success');
                    apiStatus.className = 'api-status status-configured';
                    apiStatusText.textContent = 'API key configured and saved';
                } else {
                    showTestResult('Failed to save API key: ' + data.error, 'error');
                    apiStatus.className = 'api-status status-not-configured';
                    apiStatusText.textContent = 'Failed to save API key';
                }
                
            } catch (error) {
                showTestResult('Failed to save API key: ' + error.message, 'error');
                apiStatus.className = 'api-status status-not-configured';
                apiStatusText.textContent = 'Failed to save API key';
            }
        }
        
        // AI Chat Functions
        function sendMessage() {
            const chatInput = document.getElementById('chatInput');
            const chatMessages = document.getElementById('chatMessages');
            const message = chatInput.value.trim();
            
            if (!message) return;
            
            // Add user message
            const userMessage = document.createElement('div');
            userMessage.className = 'chat-message user-message';
            userMessage.innerHTML = `
                <div class="message-header">
                    <div class="message-sender">You</div>
                    <div class="message-time">${new Date().toLocaleTimeString()}</div>
                </div>
                <div class="message-content">${escapeHtml(message)}</div>
            `;
            chatMessages.appendChild(userMessage);
            
            // Clear input
            chatInput.value = '';
            
            // Scroll to bottom
            document.getElementById('chatContainer').scrollTop = document.getElementById('chatContainer').scrollHeight;
            
            // Add typing indicator
            const typingIndicator = document.createElement('div');
            typingIndicator.className = 'chat-message ai-message typing-indicator';
            typingIndicator.innerHTML = `
                <div class="message-header">
                    <div class="message-sender">AI Error Analysis Specialist</div>
                    <div class="message-time">Analyzing...</div>
                </div>
                <div class="message-content">
                    <div class="typing-dots">
                        <span></span>
                        <span></span>
                        <span></span>
                    </div>
                </div>
            `;
            chatMessages.appendChild(typingIndicator);
            document.getElementById('chatContainer').scrollTop = document.getElementById('chatContainer').scrollHeight;
            
            // Send to AI
            sendToAI(message, typingIndicator);
        }
        
        async function sendToAI(message, typingIndicator) {
            try {
                const response = await fetch('/api/chat', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({ message })
                });
                
                const data = await response.json();
                
                // Remove typing indicator
                typingIndicator.remove();
                
                // Add AI response with enhanced formatting
                const aiMessage = document.createElement('div');
                aiMessage.className = 'chat-message ai-message';
                aiMessage.innerHTML = `
                    <div class="message-header">
                        <div class="message-sender">AI Error Analysis Specialist</div>
                        <div class="message-time">${new Date().toLocaleTimeString()}</div>
                    </div>
                    <div class="message-content">${formatAIResponse(data.response)}</div>
                `;
                document.getElementById('chatMessages').appendChild(aiMessage);
                
                document.getElementById('chatContainer').scrollTop = document.getElementById('chatContainer').scrollHeight;
                
            } catch (error) {
                typingIndicator.remove();
                showNotification('error', 'Chat Error', 'Failed to send message to AI');
            }
        }
        
        function formatAIResponse(response) {
            // Convert markdown-style formatting to HTML
            let formatted = escapeHtml(response);
            
            // Convert **Bold Blue** to blue bold text
            formatted = formatted.replace(/\*\*(.*?)\*\*/g, '<span class="blue-bold">$1</span>');
            
            // Convert **Green** to green bold text  
            formatted = formatted.replace(/\*\*(.*?)\*\*/g, '<span class="green-bold">$1</span>');
            
            // Convert **Red** to red bold text
            formatted = formatted.replace(/\*\*(.*?)\*\*/g, '<span class="red-bold">$1</span>');
            
            // Convert line breaks to <br>
            formatted = formatted.replace(/\n/g, '<br>');
            
            // Convert bullet points
            formatted = formatted.replace(/‚Ä¢ (.*)/g, '<li>$1</li>');
            formatted = formatted.replace(/<li>(.*?)<\/li>/g, '<ul><li>$1</li></ul>');
            formatted = formatted.replace(/<\/ul><ul>/g, '');
            
            // Convert numbered lists
            formatted = formatted.replace(/^\d+\. (.*)/gm, '<li>$1</li>');
            
            return formatted;
        }
        
        function clearChat() {
            const chatMessages = document.getElementById('chatMessages');
            chatMessages.innerHTML = `
                <div class="chat-message ai-message">
                    <div class="message-header">
                        <div class="message-sender">AI Error Analysis Specialist</div>
                        <div class="message-time">Just now</div>
                    </div>
                    <div class="message-content">
                        **Bold Blue** Chat cleared! I'm your specialized AI Error Analysis Assistant. I can help you with:
                        <br><br>
                        ‚Ä¢ **Green** Thorough error analysis and debugging
                        ‚Ä¢ **Green** Actionable fix suggestions  
                        ‚Ä¢ **Green** System architecture insights
                        ‚Ä¢ **Red** Security vulnerability identification
                        <br><br>
                        How can I assist you today?
                    </div>
                </div>
            `;
        }
        
        document.addEventListener('DOMContentLoaded', function() {
            // Show welcome notification
            showNotification('info', 'Dashboard Loaded', 'AI Error Analysis Dashboard v2.0.4 is ready');
            
            // Update version badge
            updateVersionBadge();
            
            // Assign global DOM elements
            errorContainer = document.getElementById('errorContainer');
            searchInput = document.getElementById('searchInput');
            refreshBtn = document.getElementById('refreshBtn');
            totalErrorsEl = document.getElementById('totalErrors');
            pendingErrorsEl = document.getElementById('pendingErrors');
            analyzedErrorsEl = document.getElementById('analyzedErrors');
            failedErrorsEl = document.getElementById('failedErrors');
            
            // Load initial errors
            loadErrors();
            
            // Check API status
            checkApiStatus();
            
            // Auto-refresh every 30 seconds
            setInterval(loadErrors, 30000);
            
            // Search functionality
            searchInput.addEventListener('input', function() {
                const searchTerm = this.value.toLowerCase();
                filteredErrors = errors.filter(error => 
                    error.appName.toLowerCase().includes(searchTerm) ||
                    error.error.toLowerCase().includes(searchTerm) ||
                    error.userInfo.toLowerCase().includes(searchTerm)
                );
                renderErrors();
            });
            
            // Refresh button
            refreshBtn.addEventListener('click', function() {
                this.disabled = true;
                this.innerHTML = '<i class="fas fa-sync-alt fa-spin"></i> Refreshing...';
                loadErrors().finally(() => {
                    this.disabled = false;
                    this.innerHTML = '<i class="fas fa-sync-alt"></i> Refresh';
                });
            });
            
            // Version tab refresh button
            const refreshVersionBtn = document.getElementById('refreshVersionBtn');
            if (refreshVersionBtn) {
                refreshVersionBtn.addEventListener('click', function() {
                    this.disabled = true;
                    this.innerHTML = '<i class="fas fa-sync-alt fa-spin"></i> Refreshing...';
                    updateVersionDetails();
                    setTimeout(() => {
                        this.disabled = false;
                        this.innerHTML = '<i class="fas fa-sync-alt"></i> Refresh';
                    }, 1000);
                });
            }
            
            // Chat functionality
            const chatInput = document.getElementById('chatInput');
            const sendMessageBtn = document.getElementById('sendMessageBtn');
            const clearChatBtn = document.getElementById('clearChatBtn');
            
            if (sendMessageBtn) {
                sendMessageBtn.addEventListener('click', sendMessage);
            }
            
            if (chatInput) {
                chatInput.addEventListener('keypress', function(e) {
                    if (e.key === 'Enter') {
                        sendMessage();
                    }
                });
            }
            
            if (clearChatBtn) {
                clearChatBtn.addEventListener('click', clearChat);
            }
        });
    </script>
