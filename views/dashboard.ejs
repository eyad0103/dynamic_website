        function updatePcTable(pcStats) {
            const tbody = document.getElementById('pcTableBody');
            tbody.innerHTML = '';
            
            Object.values(pcStats).forEach(pc => {
                const statusColor = pc.status === 'online' ? '#28a745' : '#dc3545';
                const statusIcon = pc.status === 'online' ? 'fa-check-circle' : 'fa-times-circle';
                
                const row = document.createElement('tr');
                row.style.cssText = 'border-bottom: 1px solid rgba(0, 255, 136, 0.1);';
                row.innerHTML = '<td style="padding: 0.75rem; color: #00ff88;">' + pc.pcName + '</td>' +
                    '<td style="padding: 0.75rem;">' +
                        '<span style="color: ' + statusColor + ';">' +
                            '<i class="fas ' + statusIcon + '" style="margin-right: 0.5rem;"></i>' +
                            pc.status.toUpperCase() +
                        '</span>' +
                    '</td>' +
                    '<td style="padding: 0.75rem; color: #dc3545;">' + (pc.errorCount || 0) + '</td>' +
                    '<td style="padding: 0.75rem; color: rgba(255, 255, 255, 0.6);">' +
                        (pc.lastReportSent ? new Date(pc.lastReportSent).toLocaleString() : 'Never') +
                    '</td>' +
                    '<td style="padding: 0.75rem;">' +
                        '<button onclick="viewPcDetails(\'' + pc.pcName + '\')" class="form-button" style="padding: 0.25rem 0.5rem; font-size: 0.8rem;">' +
                            '<i class="fas fa-eye"></i> View' +
                        '</button>' +
                    '</td>';
                tbody.appendChild(row);
            });
        }
        
